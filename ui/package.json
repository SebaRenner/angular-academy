    "@angular/language-service": "^18.1.3",
    "@types/jasmine": "~5.1.4",
    "@types/node": "^20.10.4",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "eslint": "^8.57.0",
    "jasmine-core": "~5.1.1",
