  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:cov": "ng test --watch=false --code-coverage=true && start ./coverage/index.html",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "analyze": "npm run build:prod -- --stats-json=true && webpack-bundle-analyzer ./dist/stats.json"
  },
  "private": true,
